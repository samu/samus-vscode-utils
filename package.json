{
  "name": "cursor-placement",
  "displayName": "cursor-placement",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-placement.pull-up",
        "title": "Pull cursor up"
      },
      {
        "command": "cursor-placement.pull-down",
        "title": "Pull cursor down"
      },
      {
        "command": "cursor-placement.move-up-and-scroll",
        "title": "Move up and scroll"
      },
      {
        "command": "cursor-placement.move-down-and-scroll",
        "title": "Move down and scroll"
      },
      {
        "command": "cursor-placement.move-selection-to-occurence",
        "title": "Move cursor"
      },
      {
        "command": "cursor-placement.vim-escape-then-run",
        "title": "Vim escape then run"
      },
      {
        "command": "cursor-placement.open-then-focus",
        "title": "Open then focus"
      },
      {
        "command": "cursor-placement.open-result-then-reset",
        "title": "Open result and reset"
      },
      {
        "command": "cursor-placement.close-find-widget-then-reset",
        "title": "Close find widget then reset"
      },
      {
        "command": "cursor-placement.revert-change-then-hide",
        "title": "Revert change then hide"
      },
      {
        "command": "cursor-placement.run-multiple",
        "title": "Run multiple"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
